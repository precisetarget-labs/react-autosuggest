{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ],
        "react",
        "stage-0"
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ],
        "react",
        "stage-0"
      ],
      "plugins": [
        [
          "transform-react-remove-prop-types", {
            "mode": "wrap"
          }
        ]
      ]
    }
  }
}
